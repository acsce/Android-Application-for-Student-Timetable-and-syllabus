<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.internshipapp">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.Profile"></activity>
        <activity android:name=".Activities.Developer" />
        <activity android:name=".Activities.About" />
        <activity android:name=".Activities.SubjectDetails" />
        <activity android:name=".Activities.SubjectActivity" />
        <activity android:name=".Activities.FacultyDetails" />
        <activity android:name=".Activities.FacultyActivity" />
        <activity android:name=".Activities.DayDetail" />
        <activity android:name=".Activities.WeekActivity" />
        <activity android:name=".Activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.HomeActivity" />
        <activity android:name=".Activities.RegisterActivity" />
    </application>

</manifest>